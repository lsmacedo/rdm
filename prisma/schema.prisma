// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RdmMigration {
  id            Int      @id @default(autoincrement())
  name          String
  type          String
  affected_rows Int
  created_at    DateTime

  @@map(name: "_rdm_migration")
}
