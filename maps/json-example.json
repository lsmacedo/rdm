{
  "name": "Local JSON Dataset Example",
  "description": "This is a sample JSON dataset of songs that may have multiple artists.",
  "source": "https://github.com/lsmacedo/rdm",
  "input": {
    "type": "json",
    "path": "example.json"
  },
  "output": {
    "type": "database",
    "tables": {
      "track": {
        "set": {
          "name": "_.tracks__name",
          "isrc": "_.tracks__isrc"
        },
        "strategy": "upsert",
        "uniqueConstraint": ["isrc"]
      },
      "artist": {
        "set": {
          "name": "_.tracks__artists__name"
        },
        "strategy": "upsert",
        "uniqueConstraint": ["name"]
      },
      "artist_track": {
        "set": {
          "artist_id": "artist.id",
          "track_id": "track.id"
        },
        "strategy": "upsert",
        "uniqueConstraint": ["artist_id", "track_id"]
      }
    }
  }
}
