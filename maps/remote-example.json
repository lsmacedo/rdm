{
  "name": "Local Remote Dataset Example",
  "description": "This is a sample dataset of locations.",
  "source": "https://documenter.getpostman.com/view/8854915/Szf7znEe",
  "input": {
    "type": "json",
    "method": "get",
    "path": "https://ghibliapi.herokuapp.com/locations"
  },
  "output": {
    "type": "database",
    "tables": {
      "location": {
        "set": {
          "name": "_.name",
          "climate": "_.climate"
        },
        "strategy": "upsert",
        "uniqueConstraint": ["name"]
      },
      "movie": {
        "set": {
          "url": "_.films",
          "location_id": "location.id"
        },
        "strategy": "upsert",
        "uniqueConstraint": ["url"]
      }
    }
  }
}
