{
  "name": "Example - Country Capitals",
  "description": "List of countries and their capitals",
  "source": "https://github.com/icyrockcom/country-capitals/blob/master/data/country-list.csv",
  "type": "local",
  "path": "example.csv",
  "entities": ["country", "city"],
  "fields": {
    "country.name": "_.country",
    "country.capital": "city.id",
    "city.name": "_.capital"
  },
  "merge": {
    "country": {
      "strategy": "upsert",
      "on": ["name"]
    },
    "city": {
      "strategy": "upsert",
      "on": ["name"]
    }
  }
}