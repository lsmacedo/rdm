{
  "name": "Alias Example",
  "description": "Example using alias instead of the full path name for each property",
  "input": {
    "file": {
      "path": "tracks.json"
    }
  },
  "output": {
    "alias": {
      "_track": "_.tracks",
      "_artist": "_.tracks.artists"
    },
    "database": {
      "url": "postgres://postgres:postgrespassword@localhost/tracks",
      "tables": {
        "track": {
          "set": {
            "name": "_track.name",
            "isrc": "_track.isrc"
          },
          "strategy": "upsert",
          "uniqueConstraint": ["isrc"]
        },
        "artist": {
          "set": {
            "name": "_artist.name"
          },
          "strategy": "upsert",
          "uniqueConstraint": ["name"]
        },
        "artist_track": {
          "set": {
            "artist_id": "artist.id",
            "track_id": "track.id"
          },
          "strategy": "upsert",
          "uniqueConstraint": ["artist_id", "track_id"]
        }
      }
    }
  }
}
