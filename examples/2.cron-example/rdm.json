{
  "name": "CRON Example",
  "description": "Example dataset to be refetched every 10 seconds",
  "cron": "*/10 * * * * *",
  "steps": [
    {
      "name": "_data",
      "description": "Read tracks from JSON file",
      "type": "file",
      "path": "tracks.json"
    },
    {
      "description": "Save rows in database",
      "type": "database",
      "url": "postgres://postgres:postgrespassword@localhost/tracks",
      "tables": {
        "track": {
          "set": {
            "name": "_data.tracks.name",
            "isrc": "_data.tracks.isrc"
          },
          "strategy": "upsert",
          "uniqueConstraint": ["isrc"]
        },
        "artist": {
          "set": {
            "name": "_data.tracks.artists.name"
          },
          "strategy": "upsert",
          "uniqueConstraint": ["name"]
        },
        "artist_track": {
          "set": {
            "artist_id": "artist.id",
            "track_id": "track.id"
          },
          "strategy": "upsert",
          "uniqueConstraint": ["artist_id", "track_id"]
        }
      }
    }
  ]
}
