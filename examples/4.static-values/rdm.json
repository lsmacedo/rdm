{
  "name": "Static Values Example",
  "description": "Example making assignment to static values",
  "steps": [
    {
      "name": "_data",
      "description": "Read rows from CSV file",
      "type": "file",
      "path": "dataset.csv"
    },
    {
      "description": "Save rows in database",
      "type": "database",
      "url": "postgres://postgres:postgrespassword@localhost/files",
      "tables": {
        "file": {
          "set": {
            "name": "_.filename",
            "size": "_.size",
            "type": "'video'"
          },
          "strategy": "upsert",
          "uniqueConstraint": ["name"]
        }
      }
    }
  ]
}
