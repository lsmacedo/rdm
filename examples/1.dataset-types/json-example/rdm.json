{
  "name": "Local JSON Dataset Example",
  "description": "Example JSON of songs with artists array.",
  "input": {
    "file": {
      "path": "tracks.json"
    }
  },
  "output": {
    "database": {
      "url": "postgres://postgres:postgrespassword@localhost/tracks",
      "tables": {
        "track": {
          "set": {
            "name": "_.tracks.name",
            "isrc": "_.tracks.isrc"
          },
          "strategy": "upsert",
          "uniqueConstraint": ["isrc"]
        },
        "artist": {
          "set": {
            "name": "_.tracks.artists.name"
          },
          "strategy": "upsert",
          "uniqueConstraint": ["name"]
        },
        "artist_track": {
          "set": {
            "artist_id": "artist.id",
            "track_id": "track.id"
          },
          "strategy": "upsert",
          "uniqueConstraint": ["artist_id", "track_id"]
        }
      }
    }
  }
}
