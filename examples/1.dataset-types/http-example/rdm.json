{
  "name": "HTTP Example",
  "description": "Example API found in the URL below",
  "source": "https://documenter.getpostman.com/view/8854915/Szf7znEe",
  "input": {
    "http": {
      "url": "https://ghibliapi.herokuapp.com/locations",
      "method": "get",
      "responseType": "json"
    }
  },
  "output": {
    "url": "postgres://postgres:postgrespassword@localhost/locations",
    "database": {
      "tables": {
        "location": {
          "set": {
            "name": "_.name",
            "climate": "_.climate"
          },
          "strategy": "upsert",
          "uniqueConstraint": ["name"]
        },
        "movie": {
          "set": {
            "url": "_.films",
            "location_id": "location.id"
          },
          "strategy": "upsert",
          "uniqueConstraint": ["url"]
        }
      }
    }
  }
}
