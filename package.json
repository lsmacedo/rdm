{
  "name": "rdm",
  "version": "0.1.0",
  "description": "RDM Dataset Mapper",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts apply $INIT_CWD",
    "rdm-init": "tsc && cp -r src/templates/* dist/templates/ && node dist/index.js init $INIT_CWD",
    "rdm-apply": "tsc && node dist/index.js apply $INIT_CWD"
  },
  "author": "Lucas Macedo",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/inquirer-fuzzy-path": "^2.3.4",
    "@types/node": "^17.0.39",
    "@types/node-cron": "^3.0.1",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^8.3.4",
    "@types/yargs": "^17.0.10",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "csv": "^6.1.0",
    "csv-string": "^4.1.0",
    "dotenv": "^16.0.1",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.4",
    "inquirer-fuzzy-path": "^2.3.0",
    "node-cron": "^3.0.1",
    "pg": "^8.7.3",
    "sql-formatter": "^6.1.2",
    "uuid": "^8.3.2",
    "yargs": "^17.5.1"
  },
  "nodemonConfig": {
    "stdin": false
  }
}
